{
  "include": ["src/**/*", "astro.config.*", ".astro/types.d.ts", "**/*"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["node_modules/@types", "./node_modules/@astrojs/client"],
    // "types": ["@astrojs/client"]
  },
}
